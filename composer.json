{
  "name": "sharelov/notification",
  "keywords": [
    "laravel",
    "notifications",
    "messages",
    "flash",
    "instant"
  ],
  "description": "Package for Laravel for helping to manage flash / instant notifications / messages.",
  "authors": [
    {
      "name": "Carlos Saldana",
      "email": "carlos@getnerdify.com"
    }
  ],
  "license": "MIT",
  "require": {
    "illuminate/support": "5.8.*|^6.0|^7.0|^8.0|^9.0",
    "illuminate/session": "5.8.*|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-0": {
      "Krucas\\Notification": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Krucas\\Notification\\NotificationServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
